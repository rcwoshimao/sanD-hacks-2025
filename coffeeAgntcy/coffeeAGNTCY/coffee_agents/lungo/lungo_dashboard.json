          "pluginVersion": "4.10.2",
          "queryType": "traces",
          "rawSql": "SELECT\n    TraceId AS traceID,\n    arrayStringConcat(groupArray(DISTINCT ServiceName), ', ') AS serviceNamesInTrace,\n    min(Timestamp) AS startTime,\n    (max(Timestamp) - min(Timestamp)) AS traceDuration\nFROM \"default\".\"otel_traces\"\nWHERE\n    Timestamp >= $__fromTime AND Timestamp <= $__toTime\n    AND (Duration > 0)\n    AND (SpanAttributes['session.id'] IS NOT NULL)\n    AND (SpanAttributes['session.id'] != '')\n    AND (SpanAttributes['session.id'] IN (${session_id:sqlstring}))\nGROUP BY\n    TraceId\nORDER BY\n    startTime ASC\nLIMIT 1000",
          "refId": "A"
        }
